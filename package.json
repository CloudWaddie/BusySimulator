{
  "name": "busy-simulator",
  "version": "0.1.0",
  "homepage": "https://CloudWaddie.github.io/BusySimulator/",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "cross-env NEXT_PUBLIC_ASSET_PREFIX='/BusySimulator' next build",
    "start": "next start",
    "lint": "next lint",
    "predeploy": "npm run build && touch out/.nojekyll",
    "deploy": "gh-pages -d out --dotfiles",
    "postdeploy": "git push --follow-tags"
  },
  "dependencies": {
    "howler": "^2.2.4",
    "next": "15.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "cross-env": "^10.0.0",
    "gh-pages": "^6.3.0",
    "tailwindcss": "^4"
  }
}
